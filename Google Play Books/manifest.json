{
   "app": {
      "background": {
         "scripts": [ "background.js" ]
      }
   },
   "name": "Google Play Books App Launcher",
   "description": "Unofficial App Launcher for Google Play Books",
   "version": "0.0.1",
   "icons": {
      "128": "assets/128.png"
   },
   "manifest_version": 2,
   "permissions": [
      "webview",
      "notifications",
      "unlimitedStorage",
      "https://*.google.com/*",
      "app.window.fullscreen.overrideEsc"
   ]
}