{
   "name":"Google Sheets",
   "version":"0.1.1",
   "manifest_version":2,
   "minimum_chrome_version":"37.0.0.0",
   "permissions":[
      "webview",
      "power",
      "storage",
      "videoCapture",
      "geolocation",
      "pointerLock",
      "system.display",
      {
         "fileSystem":[
            "write",
            "retainEntries",
            "directory"
         ]
      },
      "accessibilityFeatures.read",
      "accessibilityFeatures.modify"
   ],
   "app":{
      "background":{
         "scripts":[
            "main.js"
         ]
      }
   },
   "default_locale":"en",
   "author" : "Ryan Watts" ,
   "description": "__MSG_appDesc__",
   "icons":{
      "128":"img/128.png"
   }
}
